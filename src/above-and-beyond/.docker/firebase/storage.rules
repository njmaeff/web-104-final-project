rules_version = '2';
service firebase.storage {
  match /b/{bucket}/o/{userId} {
    match /{allPaths=**} {
      allow read, write: if request.auth.uid == userId
    }
  }
  match /b/{bucket}/o/exports/{userId} {
    match /{allPaths=**} {
      allow read, write: if request.auth.uid == userId
    }
  }
}
